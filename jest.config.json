{
  "testEnvironment": "node",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "moduleNameMapper": {
    "^(\\.{1,2}/.*)\\.js$": "$1"
  },
  "testRegex": "/__tests__/(.*(\\.|)(test|spec))\\.ts$",
  "coverageDirectory": "coverage",
  "collectCoverageFrom": [
    "src/**/*.{ts,tsx,js,jsx}",
    "!src/**/*.d.ts",
    "!src/models/**/*.ts"
  ]
}
